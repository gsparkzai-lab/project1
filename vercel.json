            "source": "/(api|static|favicon.ico|_next|__next|.*\\.js|.*\\.css|.*\\.png|.*\\.jpg|.*\\.svg|.*\\.json|.*\\.txt)",
            "destination": "/$1"
        },
        {
            "source": "/(.*)",
